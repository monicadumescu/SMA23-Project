<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Agentie_Imobiliara" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/material_dynamic_primary70</item>
        <item name="colorPrimaryVariant">@color/material_dynamic_primary70</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/material_dynamic_primary70</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Agentie_Imobiliara.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Agentie_Imobiliara.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Agentie_Imobiliara.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>